<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">

    <style name="mediumTextStyle" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textColor">@color/greyTeal</item>
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:lineSpacingExtra">2sp</item>
    </style>

    <style name="boldTextStyle" parent="TextAppearance.AppCompat">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">@dimen/_13ssp</item>
        <item name="android:textColor">@color/greyTeal</item>
        <item name="fontFamily">@font/inter_bold</item>
        <item name="android:textFontWeight" tools:targetApi="p">400</item>
        <item name="android:lineSpacingExtra">2sp</item>
    </style>
</resources>